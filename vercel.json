{
  "functions": {
    "backend/app/main.py": {
      "runtime": "@vercel/python@3.11"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/index.html"
    }
  ],
  "env": {
    "APP_MODULE": "main:app"
  },
  "buildCommand": "cd frontend && npm install && npm run build && cd ../backend && pip install -r requirements.txt"
}
